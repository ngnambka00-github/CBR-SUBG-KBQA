--logging_steps=10 \
--loss_metric=txent \
--max_grad_norm=1 \
--num_gcn_layers=3 \
--num_neighbors_eval=5 \
--num_neighbors_train=10 \
--num_train_epochs=5 \
--train_batch_size=4 \
--output_dir=/home/namnv/FTechProject/CBR-SUBG/src/private_models/custom \
--temperature=0.06453104622317246 \
--transform_input=1 \
--use_fast_rgcn=1 \
--use_wandb=0

