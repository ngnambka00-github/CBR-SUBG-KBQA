4. runner train
python src/runner.py \
--do_train \
--do_predict \
--add_dist_feature=1 \
--add_inv_edges_to_edge_index=1 \
--data_dir=/home/namnv/FTechProject/CBR-SUBG/adaptive_subgraph_collection/data/subgraph \
--data_file_suffix=roberta-base_mean_pool_masked_cbr_subgraph_k=10 \
--dataset_name=metaqa \
--dist_metric=cosine \
